#include <cstdio>
#include <iostream>
#include <string>
#include <algorithm>
#include <ctime>

using namespace std;

typedef unsigned long long llu;

int main( void )
{
	freopen("test.in","wt",stdout );
	srand( time( NULL ) );
	int N = 100000;
	printf("100000\n");
	for( int i = 1; i <= N; i++ ) {
		string name;
		for( int j = 1; j <= 20; j++ ) {
			name += ( rand() % 26 + 'a' );
		}
		llu u,v;
		u = i;
		v = 1000000;
		cout << "A " << name << " " << u << " " << v << endl;
	}
	return 0;
}
