#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	freopen("test.in","wt",stdout);
	int T = rand() % 10;
	while( T-- ) {
		int N = rand() % 100000 + 1;
		printf("%d\n", N );
		for( int i = 1; i <= N; i++ ) {
			int u = rand() % 1000000000 + 1;
			printf("%d ", u );	
		}
		printf("\n");
	}
	printf("0\n");
	return 0;
}
