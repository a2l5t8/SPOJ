#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	freopen("test.in","wt",stdout);
	srand( time( NULL ) );
	int N, U, Q;
	N = rand() % 100000000 + 1;
	U = rand() % 1000 + 1;
	Q = rand() % 1000 + 1;
	printf("%d %d %d\n", N, U, Q );
	while( U-- ) {
		int u = rand() % 10000 + 1;
		int v = rand() % 10000 + 1;
		if( u > v ) swap( u, v );
		printf("%d %d\n", u, v );
	}
	while( Q-- ) {
		int u = rand() % 10000 + 1;
		int v = rand() % 10000 + 1;
		if( u > v ) swap( u, v );
		printf("%d %d\n", u, v );
	}
	return 0;
}
