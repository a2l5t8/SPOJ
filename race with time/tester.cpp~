#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	freopen("test.in", "wt", stdout );
	int N = rand() % 1000 + 1;
	int Q = rand() % 1000 + 1;
	printf("%d %d\n", N, Q );
	for( int i = 1; i <= N; i++ ) {
		printf("%d\n", rand() % 1000000000 );
	}
	for( int i = 1; i <= Q; i++ ) {
		bool u = rand() % 2;
		if( u == 0 ) {
			printf("M ");
			int k,l;
			k = rand() % N + 1;
			l = rand() % 1000000000;
			printf("%d %d\n", k, l );
		} else {
			printf("C ");
			int k,l,m;
			k = rand() % N + 1;
			l = rand() % N + 1;
			m = rand() % 1000000000;
			if( k > l ) swap( k, l );
			printf("%d %d %d\n", k, l, m );
		}
	}
	return 0;
}
