#include <cstdio>
#include <algorithm>

#define MAXN 10000000
#define MOD 10000LL

using namespace std;

typedef long long ll;

ll A[ MAXN + 1 ];

int main( void )
{
	//freopen("test.in","rt",stdin);
	//freopen("test2.out","wt",stdout);
	int N, U, Q, u, v;
	scanf("%d%d%d", &N, &U, &Q );
	while( U-- ) {
		scanf("%d%d", &u, &v );
		for( int i = u; i <= v; i++ ) {
			A[ i ] = ( ll )( A[ i ] + ( ll )( i - u + 1 ) ) % MOD;
		}
	}
	while( Q-- ) {
		scanf("%d%d", &u, &v );
		ll sum = 0;
		for( int i = u; i <= v; i++ ) {
			sum = ( sum + A[ i ] ) % MOD;
		}
		printf("%lld\n", sum );
	}
	return 0;
}
