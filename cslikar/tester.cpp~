#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	int R, C;
	R = rand() % 50 + 1;
	C = rand() % 50 + 1;
	int firex, firey, destx, desty, startx, starty;
	firex = rand() % R + 1;
	firey = rand() % C + 1;
	destx = rand() % R + 1;
	desty = rand() % C + 1;
	startx = rand() % R + 1;
	starty = rand() % C + 1;
	printf("%d %d\n", R, C );
	for( int i = 1; i <= R; i++ ) {
		for( int j = 1; j <= C; j++ ) {
			if( i == firex && j == firey ) printf("*");
			else if( i == destx && j == desty ) printf("D");
			else if( i == startx && j == starty ) printf("S");
			else {
				int c = rand() % 10;
				if( c <= 2 ) printf(".");
				else printf("X");
			}
		}
		printf("\n");
	}
	return 0;
}
