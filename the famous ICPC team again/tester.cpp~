#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	freopen("test.in","wt",stdout);
	srand( time( NULL ) );
	int T = rand() % 5;
	while( T-- ) {
		int N = 100000;
		printf("%d\n", N );
		for( int i = 1; i <= N; i++ ) {
			int u = rand() % 1000000000 + 1;
			printf("%d ", u );
		}
		printf("\n");
		int Q = 100000;
		printf("%d\n", Q );
		for( int i = 1; i <= Q; i++ ) {
			int u = rand() % N + 1, v = rand() % N + 1;
			if( u > v ) swap( u, v );
			printf("%d %d\n", u, v );
		}	
	}
	return 0;
}
