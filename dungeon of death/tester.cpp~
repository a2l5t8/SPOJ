#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	freopen("test.in","wt",stdout);
	int T = rand() % 20 + 1;
	printf("%d\n", T );
	while( T-- ) {	
		int N = rand() % 100 + 1;
		printf("%d\n", N );
		for( int i = 0; i < N; i++ ) {
			int u = rand() % 120;
			int v = rand() % 120;
			printf("%d %d\n", u, v );
		}
	}
	return 0;
}
