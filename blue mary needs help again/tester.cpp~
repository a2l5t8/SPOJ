#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	freopen("test.in","wt",stdout );
	char chars[] = { 'I', 'S', 'A', 'F' };
	int T;
	T = 1;
	printf("%d\n", T );
	while( T-- ) {
		int Q,minim;
		Q = 20000;
		minim = rand() % 100001;
		printf("%d %d\n", Q, minim );
		while( Q-- ) {
			int c = rand() % 4;
			printf("%c", chars[ c ] );
			if( chars[ c ] == 'I' ) {
				int v = rand() % 100001;
				printf(" %d\n", v );
			}
			else if( chars[ c ] == 'S' ) {
				int v = rand() % 1001;
				printf(" %d\n", v )
;			}
			else if( chars[ c ] == 'A' ) {
				int v = rand() % 1001;
				printf(" %d\n", v );
			}
			else {
				int v = rand() % 10 + 1;
				printf(" %d\n", v );
			}
		}
	}
	return 0;
}
