#include <cstdio>
#include <algorithm>
#include <ctime>

using namespace std;

int main( void )
{
	freopen("test.in","wt",stdout);
	srand( time( NULL ) );
	int N = 100, Q = 100;
	printf("%d %d\n", N, Q );
	for( int i = 1; i <= N; i++ ) {
		int u = rand() % 10000 + 1;
		printf("%d ", u );
	}
	while( Q-- ) {
		int t, x, l, u, v;
		t = rand() % 2;
		printf("%d", t + 1 );
		if( t == 0 ) {
			int l = rand() % 10000 + 1;
			int u = rand() % N + 1;
			printf(" %d %d\n", l, u );
		} else {
			x = rand() % 3;
			if( x == 0 ) printf(" XOR");
			else if( x == 1 ) printf(" OR");
			else printf(" AND");
			int u = rand() % N + 1, v = rand() % N + 1;
			printf(" %d %d\n", min( u, v ), max( u, v ) );
		}
	}
	return 0;
}
