#include <cstdio>
#include <ctime>
#include <cstdlib>
#include <algorithm>

using namespace std;

int main( void )
{
	srand( time( NULL ) );
	freopen("test.in","wt",stdout);
	int n = 100000;
	int q = 500000;
	printf("%d %d\n", n, q );
	for( int i = 1; i <= n; i++ ) {
		printf("%d ", rand() % 1000000001 );
	}
	printf("\n");
	for( int i = 1; i <= q; i++)  {
		int q = rand() % 3 + 1;
		printf("%d ", q );
		if( q == 1 ) {
			printf("%d\n", rand() % n + 1 );
		} else {
			printf("%d\n", rand() % 1000000000 + 1 );
		}
	}
	return 0;
}
