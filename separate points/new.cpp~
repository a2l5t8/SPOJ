#include <cstdio>
#include <cstdlib>

using namespace std;

int main( void )
{
	freopen("test.in","rt",stdin);
	//while( true ) {
		freopen("test1.in","wt",stdout);
		int N,M,x,y;	
		scanf("%d%d", &N, &M );
		printf("%d %d\n", N, M );
		if( !N && !M ) {
			break;
		}	
		for( int i = 0; i < N; i++ ) {
			scanf("%d%d", &x,&y );
			printf("%d %d\n", x,y );
		}
		for( int j = 0; j < M; j++ ) {
			scanf("%d%d", &x,&y );
			printf("%d %d\n", x, y );
		}
		system("./spoints");
	//}
	return 0;
}
