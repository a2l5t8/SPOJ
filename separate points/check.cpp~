#include <iostream>
#include <fstream>
#include <string>

using namespace std;

int main( void )
{
	ifstream ii("test.out");
	ifstream io("correct.out");
	int i;
	string test[ 1000 ],news;
	for( i = 0; ii >> test[ i ]; i++ );
	for( int j = 0; j < i; j++ ) {
		io >> news;
		if( news != test[ j ] ) {
			cout << "in line " << j + 1 << " expected " << news << " got " << test[ j ] << endl;
		}
	} 
	return 0;
}
