#include <cstdio>
#include <algorithm>

using namespace std;

int main( void )
{
	int s, cnt = 0;
	scanf("%d", &s );
	freopen("test.in","rt",stdin);
	while( s-- ) {
		int n,m;
		scanf("%d%d",&n,&m);
		cnt++;
		for( int i = 0; i < n; i++ ) {
			scanf("%*d%*d" );
			cnt++;
		}
		for( int j = 0; j < m; j++ ) {
			scanf("%*d%*d");
			cnt++;
		}
	}
	printf("line -> %d\n", cnt );
	return 0;
}
