#include <cstdio>
#include <ctime>
#include <algorithm>

using namespace std;

int main( void )
{
	freopen("test.in","wt",stdout);
	//srand( time( NULL ) );
	int T = rand() % 10;
	printf("%d\n", T );
	for( int i = 1; i <= T; i++ ) {
		int N = 40000;
		printf("%d\n", N );
		for( int i = 1; i <= N; i++ ) {	
			int u = rand() % 10000000 + 1;
			int v = rand() % 10000000 + 1;
			if( u > v ) swap( u,v );
			printf("%d %d\n", u, v );
		}
	}
	return 0;
}
