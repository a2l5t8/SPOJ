#include <cstdio>
#include <algorithm>
#include <ctime>

#define MAXN 200000

using namespace std;

int main( void )
{
	freopen("test.in","wt",stdout);
	srand( time( NULL ) );
	int N = rand() % MAXN + 1;
	printf("%d\n", N );
	for( int i = 1; i <= N; i++ ) {
		int x = rand() % 10000000, y = rand() % 10000000;
		printf("%d %d\n", x, y );
	}
	return 0;
}
